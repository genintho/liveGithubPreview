{
    "name": "Live Github Preview",
    "description": "RealTime Markdown preview for new Issues",
    "version": "1.0",
    "permissions": ["contextMenus"],
    "background": {
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://github.com/*/*/issues/*"],
            "js": ["livemarkdown.js", "contentScript.js"]
        }
    ]
}